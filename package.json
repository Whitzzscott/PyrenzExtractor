{
  "name": "pyrenzextractor",
  "version": "1.0.0",
  "description": "",
  "main": "tailwind.config.js",
  "type": "commonjs",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build:ts": "esbuild src/content.ts --bundle --outfile=dist/content.js --target=es2020 --platform=browser",
    "build:css": "npx @tailwindcss/cli -i ./public/index.css -o ./dist/index.css --minify",
    "copy:static": "powershell -Command \"Copy-Item -Path public\\* -Destination dist\\ -Recurse -Force\"",
    "copy:manifest": "powershell Copy-Item -Path manifest.json -Destination dist\\",
    "build": "concurrently \"npm run build:ts\" \"npm run build:css\" \"npm run copy:static\" \"npm run copy:manifest\"",
    "watch:ts": "esbuild src/content.ts --bundle --outfile=dist/content.js --target=es2020 --platform=browser --watch",
    "watch:css": "npx @tailwindcss/cli -i ./public/index.css -o ./dist/index.css --watch",
    "dev": "concurrently \"npm run watch:ts\" \"npm run watch:css\""
  },
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "autoprefixer": "^10.4.21",
    "browserslist": "^4.25.1",
    "caniuse-lite": "^1.0.30001726",
    "electron-to-chromium": "^1.5.179",
    "escalade": "^3.2.0",
    "fraction.js": "^4.3.7",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.11",
    "node-releases": "^2.0.19",
    "normalize-range": "^0.1.2",
    "picocolors": "^1.1.1",
    "postcss": "^8.5.6",
    "postcss-value-parser": "^4.2.0",
    "source-map-js": "^1.2.1",
    "update-browserslist-db": "^1.1.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "concurrently": "^8.2.2",
    "esbuild": "^0.21.5",
    "nodemon": "^3.1.10",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.4.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
