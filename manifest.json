{
    "manifest_version": 3,
    "name": "Pyrenz Extractor",
    "version": "1.0",
    "permissions": ["scripting"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_idle"
      }
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "web_accessible_resources": [
      {
        "resources": ["index.css"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  